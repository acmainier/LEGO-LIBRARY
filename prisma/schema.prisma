generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LegoSet {
  id         Int     @id @default(autoincrement())
  name       String
  ownedBy    String
  year       Int
  pieceCount Int
  theme      String
  imageUrl   String?
}
